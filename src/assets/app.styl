body {
  background-color #fff
}
.pages-ctn{
  background-color #F3F6F9
  // padding-bottom 60px
}
.page-container{
  background-color #fff
  margin-bottom 20px
  border-radius 15px
  padding: 25px 15px
}
.top-container{
  border-radius 0px 0px 15px 15px
}
.small-btn{
  width 115px
  height 30px
  text-align center
  line-height 30px
  border-radius 15px
  font-size 14px
  display inline-block
}
.middle-btn{
  width 165px
  height 48px
  text-align center
  line-height 48px
  border-radius 24px
  font-size 18px
  display inline-block
}
.big-btn{
  width 100%
  height 48px
  text-align center
  line-height 48px
  border-radius 24px
  font-size 18px
}
.default-btn{
  background-color #FE5806
  color #fff
}
.white-btn{
  background-color #fff
  color #FE5806
  border 1px solid #FE5806
}
.disabled-btn{
  background-color #FFDAC7
  color #fff
}
.container-title{
  display flex
  align-items center
  .title-icon{
    width 17px
    height 17px
    vertical-align middle
    margin-right 8px
    margin-top -1px
  }
  .arrow-icon{
    width 10px
    height 5px
    margin-right 4px
    margin-left 4px
  }
  .date{
    color #FE5806
    font-size 16px
    font-weight 500
  }
  .title{
    font-weight 500
    color #353536
    font-size 16px
    vertical-align middle
  }
}
.my-content{
  padding 10px 15px
}
.cube-checkbox_checked .cubeic-right{
  color: #fc9153 !important;
}
.border-bottom-1px::after{
  border-bottom: none;
}